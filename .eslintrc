{
  "parser": "babel-eslint",
  "extends": [
    "airbnb/base",
    "plugin:import/errors"
  ],
  "rules": {
    "no-use-before-define": 0,
    "arrow-body-style": 0,
    "dot-notation": 0,
    "no-console": 0
  },
  "env": {
    "mocha": true,
    "jest": true
  },
  "globals": {
    "client": true
  }
}
